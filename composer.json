{
    "name": "gzero/api",
    "description": "GZERO API",
    "keywords": [
        "laravel",
        "cms",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Skierniewski",
            "email": "adrian.skierniewski@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-cors": "0.8.2",
        "flow/jsonpath": "^0.3.4",
        "gzero/core": "dev-master",
        "gzero/entity": "dev-master",
        "gzero/repository": "dev-master",
        "gzero/validator": "dev-master",
        "illuminate/pagination": "5.3.*",
        "illuminate/routing": "5.3.*",
        "laravel/passport": "^1.0",
        "league/fractal": "0.12.0"
    },
    "require-dev": {
        "aedart/testing-laravel": "1.8.*",
        "codeception/codeception": "2.2.*",
        "fzaninotto/faker": "~1.6",
        "gzero/testing": "2.*",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "2.*",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Gzero\\": "src/Gzero"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "./vendor/bin/codecept run"
    },
    "minimum-stability": "stable"
}
